@{
	Layout = null;
}

<!doctype html>
<html lang="en" dir="ltr">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<title>SI - SuPro | Submission System for MCC</title>

	<!-- Favicon -->
	<link rel="shortcut icon" href="~/hope-ui-html-1.2.0/html/assets/images/favicon.ico" />

	<!-- Library / Plugin Css Build -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/core/libs.min.css" />


	<!-- Hope Ui Design System Css -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/hope-ui.min.css?v=1.2.0" />

	<!-- Custom Css -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/custom.min.css?v=1.2.0" />

	<!-- Dark Css -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/dark.min.css" />

	<!-- Customizer Css -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/customizer.min.css" />

	<!-- RTL Css -->
	<link rel="stylesheet" href="~/hope-ui-html-1.2.0/html/assets/css/rtl.min.css" />

</head>
<body class=" " data-bs-spy="scroll" data-bs-target="#elements-section" data-bs-offset="0" tabindex="0">
	<!-- loader Start -->
	<div id="loading">
		<div class="loader simple-loader">
			<div class="loader-body"></div>
		</div>
	</div>
	<!-- loader END -->

	<div class="wrapper">
		<section class="login-content">
			<div class="row m-0 align-items-center bg-white vh-100">
				<div class="col-md-6 d-md-block d-none bg-primary p-0 mt-n1 vh-100 overflow-hidden">
					<img src="~/hope-ui-html-1.2.0/html/assets/images/auth/05.png" class="img-fluid gradient-main animated-scaleX" alt="images">
				</div>
				<div class="col-md-6">
					<div class="row justify-content-center">
						<div class="col-md-10">
							<div class="card card-transparent auth-card shadow-none d-flex justify-content-center mb-0">
								<div class="card-body">
									<a href="/" class="navbar-brand d-flex align-items-center mb-3">
										<!--Logo start-->
										<svg width="30" class="" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
											<img src="~/logo supro.png" width="30" height="30" />
										</svg>
										<!--logo End-->                              <h4 class="logo-title ms-3">SI - SuPro</h4>
									</a>
									<h2 class="mb-2 text-center">Sign Up</h2>
									<p class="text-center">----------------</p>
									<form role="form" method="POST" action="Login/Register" id="formRegister" class="row g-3 needs-validation" novalidate>
										<div class="row">
											<div class="col-lg-6">
												<div class="form-group">
													<label for="nik" class="form-label">NIK</label>
													<input name="NIK" type="text" class="form-control" id="nik" placeholder=" " required>
													<div class="invalid-feedback">
														Please input NIK.
													</div>
												</div>
											</div>
											<div class="col-lg-6">
												<div class="form-group">
													<label for="full_name" class="form-label">Full Name</label>
													<input name="Fullname" type="text" class="form-control" id="full_name" placeholder=" " required>
													<div class="invalid-feedback">
														Please input Fullname.
													</div>
												</div>
												
											</div>
											<div class="col-lg-6">
												<label for="classmcc" class="form-label">Class MCC</label>
												<select name="ClassID" class="form-control" id="clasmcc" required>	
													<option value=""></option>
													<option value="1">Java</option>
													<option value="2">.NET</option>
												</select>
												<div class="invalid-feedback">
													Please select a class.
												</div>
											</div>
											<div class="col-lg-6">
												<div class="form-group">
													<label for="batch" class="form-label">Batch</label>
													<input name="Batch" type="text" class="form-control" id="batch" placeholder=" " required>
													<div class="invalid-feedback">
													Please input batch.
												</div>
												</div>
											</div>
											<div class="col-lg-6">
												<label for="validationCustomUsername" class="form-label">Username</label>
												<div class="form-group input-group">
													<span class="input-group-text" id="basic-addon1">@@</span>
													<input name="Username" type="text" class="form-control" id="validationCustomUsername" aria-label="Username" aria-describedby="basic-addon1" required>
													<div class="invalid-feedback">
														Please input a username.
													</div>
												</div>
											</div>
											<div class="col-lg-6">
												<div class="form-group">
													<label for="email" class="form-label">Email</label>
													<input name="Email" type="email" class="form-control" id="email" placeholder=" " required>
													<div class="invalid-feedback">
														Please input an email.
													</div>
												</div>
											</div>
											<div class="col-lg-6">
												<div class="form-group">
													<label for="password" class="form-label">Password</label>
													<input name="Password" type="password" class="form-control" id="password" placeholder=" " required>
													<div class="invalid-feedback">
														Please input a password.
													</div>
												</div>									
											</div>
											@*<div class="col-lg-6">
												<div class="form-group">
													<label for="confirm-password" class="form-label">Confirm Password</label>
													<input type="text" class="form-control" id="confirm-password" placeholder=" ">
												</div>
											</div>*@
											<div class="col-12">
												<div class="form-check">
													<input class="form-check-input" type="checkbox" value="" id="invalidCheck" required>
													<label class="form-check-label" for="invalidCheck">
														Agree to terms and conditions
													</label>
													<div class="invalid-feedback">
														You must agree before submitting.
													</div>
												</div>
											</div>
										</div>
										<div class="d-flex justify-content-center">
											<button type="submit" class="btn btn-primary">Sign Up</button>
										</div>
										<p class="mt-3 text-center">
											Already have an Account <a href="/login" class="text-underline">Sign In</a>
										</p>
									</form>
								</div>
							</div>
						</div>
					</div>
					<div class="sign-bg sign-bg-right">
						<svg width="280" height="230" viewBox="0 0 421 359" fill="none" xmlns="http://www.w3.org/2000/svg">
							<g opacity="0.05">
								<rect x="-15.0845" y="154.773" width="543" height="77.5714" rx="38.7857" transform="rotate(-45 -15.0845 154.773)" fill="#3A57E8" />
								<rect x="149.47" y="319.328" width="543" height="77.5714" rx="38.7857" transform="rotate(-45 149.47 319.328)" fill="#3A57E8" />
								<rect x="203.936" y="99.543" width="310.286" height="77.5714" rx="38.7857" transform="rotate(45 203.936 99.543)" fill="#3A57E8" />
								<rect x="204.316" y="-229.172" width="543" height="77.5714" rx="38.7857" transform="rotate(45 204.316 -229.172)" fill="#3A57E8" />
							</g>
						</svg>
					</div>
				</div>
			</div>
		</section>
	</div>

	<!-- Library Bundle Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/core/libs.min.js"></script>

	<!-- External Library Bundle Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/core/external.min.js"></script>

	<!-- Widgetchart Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/charts/widgetcharts.js"></script>

	<!-- mapchart Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/charts/vectore-chart.js"></script>
	<script src="~/hope-ui-html-1.2.0/html/assets/js/charts/dashboard.js"></script>

	<!-- fslightbox Script -->
	<script src="~/hope-ui-html-1.2.0/html./assets/js/plugins/fslightbox.js"></script>

	<!-- Settings Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/plugins/setting.js"></script>

	<!-- Slider-tab Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/plugins/slider-tabs.js"></script>

	<!-- Form Wizard Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/plugins/form-wizard.js"></script>

	<!-- AOS Animation Plugin-->
	<!-- App Script -->
	<script src="~/hope-ui-html-1.2.0/html/assets/js/hope-ui.js" defer></script>
	<script>
		var message = '@TempData["message"]';
	</script>
	<script src="~/js/login.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.6.16/dist/sweetalert2.all.min.js"</script>
</body>
</html>