@{
	Layout = "_LayoutTrainer";
}

<div class="section-content text-center mb-5!important" style="font-family : 'Oleo Script',cursive; color:#fcc500;">
	<h1 class="section-header"><span class="content-header wow fadeIn " data-wow-delay="0.2s" data-wow-duration="2s"> Submission Project</span></h1>
</div>
<form>
	<div class="col-md-6 form-line">
		<div class="form-group">
			<label>Project Title</label>
			<input type="text" class="form-control" id="re_title" placeholder="Project Title" disabled>
		</div>
		<div class="form-group">
			<label>Description</label>
			<input type="text" class="form-control" id="re_description" placeholder="Description" disabled>
		</div>
		<div class="form-group">
			<label>NIK</label>
			<input type="text" class="form-control" id="re_nik" placeholder="NIK" disabled>
		</div>
		<div class="form-group">
			<label>Name</label>
			<input type="text" class="form-control" id="re_name" placeholder="Name" disabled>
		</div>
		<div class="form-group">
			<label>Email</label>
			<input type="text" class="form-control" id="re_email" placeholder="Email" disabled>
		</div>
		<div class="form-group">
			<label>Class</label>
			<input type="text" class="form-control" id="re_class" placeholder="Class" disabled>
		</div>
		<div class="form-group">
			<label>Batch</label>
			<input type="text" class="form-control" id="re_batch" placeholder="Batch" disabled>
		</div>
		<div class="form-group">
			<label>Current Status</label>
			<input type="text" class="form-control" id="re_currentstatus" placeholder="Current Status" disabled>
		</div>
	</div>
	<div class="col-md-6">
		<div class="form-group">
			<label for="status">Status</label>
			<select class="form-select" name="status" id="status">
				<option selected disabled value="">Choose...</option>
				<option value="2"> Revisi </option>
				<option value="3"> Diterima/Lanjutkan Pengerjaan </option>
				<option value="4"> Ditolak </option>
			</select>
		</div>
		<div class="form-group">
			<label for="revision">Revision</label>
			<input type="text" class="form-control" id="revision" placeholder="Enter Your Revison">
		</div>
		<div class="form-group">
			<div class="form-group">
				<label for="message">Message</label>
				<textarea class="form-control" id="message" placeholder="Enter Your Message"></textarea>
			</div>
			<div>

				<button type="button" onclick="PostReview()" class="btn btn-info submit"><i class="fa fa-paper-plane" aria-hidden="true"></i>  Send Message</button>
			</div>
		</div>
</form>
		</div>



